    "labelsOn": {
        "type": "switch",
        "default": true,
        "description": "Labels",
        "value": true
    },
    "borderOn": {
        "type": "switch",
        "default": false,
        "description": "Add a border to each graph",
        "value": false
    },
    "graphSpacing": {
        "type": "scale",
        "default": 1,
        "description": "Graph spacing",
        "min": -1,
        "max": 10,
        "step": 1,
        "show-value": true,
        "value": 1
    },
    "thickness": {
        "type": "scale",
        "default": 1,
        "description": "Curves thickness",
        "min": 1,
        "max": 5,
        "step": 1,
        "show-value": true,
        "value": 1
    },
    "useIconSize": {
        "type": "switch",
        "default": false,
        "description": "Limit height to the size of a colored icon",
        "value": false
    },
    "graphHeight": {
        "type": "scale",
        "default": 40,
        "description": "Graph height (pixels)",
        "min": 16,
        "max": 64,
        "step": 4,
        "show-value": true,
        "dependency": "!useIconSize",
        "value": 40
    },
    "refreshRate": {
        "type": "scale",
        "default": 1250,
        "description": "Refresh Rate (ms)",
        "min": 500,
        "max": 3000,
        "step": 50,
        "show-value": true,
        "value": 1250
    },
    "labelColor": {
        "type": "colorchooser",
        "default": "rgba(238, 238, 236, 1)",
        "description": "Label Color",
        "value": "rgba(238, 238, 236, 1)"
    },
    "borderColor": {
        "type": "colorchooser",
        "default": "rgba(238, 238, 236, 1)",
        "description": "Border Color",
        "dependency": "borderOn",
        "value": "rgba(238, 238, 236, 1)"
    },
    "backgroundColor": {
        "type": "colorchooser",
        "default": "rgba(48, 48, 48, 0.1)",
        "description": "Background Color",
        "value": "rgba(48, 48, 48, 0.1)"
    },
    "percentAtEndOfLine": {
        "type": "switch",
        "default": false,
        "description": "Place % at the very end of the line",
        "value": false
    },
    "removeEnlightenment": {
        "type": "button",
        "dependency": "isHighlighted",
        "callback": "_removeEnlightenment",
        "description": "Remove Enlightenment of this applet"
    },
    "CPU_enabled": {
        "type": "switch",
